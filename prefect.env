# Prefect Configuration for Quantum Patterns Analysis

# Use local SQLite database for simplicity
PREFECT_API_DATABASE_CONNECTION_URL="sqlite+aiosqlite:///./prefect.db"

# Enable local development mode
PREFECT_UI_URL="http://localhost:4200"

# Set log level
PREFECT_LOGGING_LEVEL="INFO"

# Enable flow run retries
PREFECT_FLOW_RUN_RETRY_DELAY_SECONDS=30

# Set task concurrency
PREFECT_TASK_RUN_CONCURRENCY_LIMIT=4

